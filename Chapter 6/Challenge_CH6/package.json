{
  "name": "challenge_ch6",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon app.js",
    "db:create": "npx sequelize-cli db:create",
    "model-gen:user": "npx sequelize-cli model:generate --name user_game --attributes username:string,password:string",
    "model-gen:biodata": "npx sequelize-cli model:generate --name user_game_biodata --attributes firstName:string,lastName:string,email:string",
    "model-gen:history": "npx sequelize-cli model:generate --name user_game_history --attributes score:integer,lastGame:string",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate-down": "npx sequelize-cli db:migrate:undo",
    "seed": "npx sequelize-cli db:seed:all",
    "seed-gen:user": "npx sequelize-cli seed:generate --name user_game",
    "seed-gen:biodata": "npx sequelize-cli seed:generate --name user_game_biodata",
    "seed-gen:history": "npx sequelize-cli seed:generate --name user_game_history",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
